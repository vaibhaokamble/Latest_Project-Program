  </servlet>
  <servlet-mapping>
  	<servlet-name>SecondServlet</servlet-name>
  	<url-pattern>/second</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AdditionServlet</servlet-name>
  	<url-pattern>/addition</url-pattern>
  </servlet-mapping>
